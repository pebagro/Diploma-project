@model Shift

@using (Html.BeginForm())
{
    @Html.Hidden("EmployeeModelId", (int)ViewBag.EmployeeModelId)
    <div class="form-group">
        <label for="StartTime">Rozpoczęcie zmiany</label>
        @Html.EditorFor(model => model.StartTime, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <label for="EndTime">Zakończenie zmiany</label>
        @Html.EditorFor(model => model.EndTime, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <button type="submit" class="btn btn-primary">Dodaj zmiane</button>
}